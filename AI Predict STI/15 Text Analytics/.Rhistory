library(data.table)
library(rpart)
library(rpart.plot)	# for enhanced tree plots
set.seed(2020)		# for randomisation in 10-fold CV
options(digits = 5)	# reduce the no. of decimals
options(scipen = 999)	# remove all e notation
m2 <- rpart(Price ~ ., data = data1, method = 'anova', control = rpart.control(minsplit = 2, cp = 0))
# use method = 'anova' if continuous Y
# by default, cp = 0.01
rpart.plot(m2, nn = T, main = 'Chart Name') # tree plotting
CVerror.cap <- m2$cptable[which.min(m2$cptable[,'xerror']), 'xerror'] + m2$cptable[which.min(m2$cptable[,'xerror']), 'xstd'] # min CV + 1 SE
i <- 1; j <- 4
while (m2$cptable[i,j] > CVerror.cap) {
i <- i + 1
}
cp.opt = ifelse(i > 1, sqrt(m2$cptable[i,1] * m2$cptable[i-1, 1]), 1)
m2.1 <- prune(m2, cp = cp.opt)
print(m2.1)
